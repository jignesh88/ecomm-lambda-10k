# AWS Configuration
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1
CDK_DEFAULT_ACCOUNT=${AWS_ACCOUNT_ID}
CDK_DEFAULT_REGION=${AWS_REGION}

# Database
DATABASE_URL=postgresql://username:password@cluster-endpoint:5432/ecommerce
DB_CLUSTER_ARN=arn:aws:rds:us-east-1:123456789012:cluster:ecommerce-cluster
DB_SECRET_ARN=arn:aws:secretsmanager:us-east-1:123456789012:secret:rds-db-credentials
DB_NAME=ecommerce

# Redis
REDIS_ENDPOINT=your-redis-cluster.cache.amazonaws.com
REDIS_PORT=6379

# Cognito
USER_POOL_ID=us-east-1_XXXXXXXXX
USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Environment
NODE_ENV=development
LOG_LEVEL=debug

# Optional: Custom Domain
DOMAIN_NAME=api.yourstore.com
HOSTED_ZONE_ID=Z123456789ABCDEFGH